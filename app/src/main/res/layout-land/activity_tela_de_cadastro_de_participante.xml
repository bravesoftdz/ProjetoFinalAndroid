<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="projetofinal.celer.com.br.projetofinal.CadastroDeParticipante.TelaDeCadastroDeParticipanteActivity">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"

            >

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeGrupo_tvId"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/telaDeCadastroDeGrupo_edtId" />

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvNome"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeUsuario_tvNome"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtNomeUsuario"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/TelaDeCadastroDeUsuario_edtNomeUsuario" />

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvEndereco"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeUsuario_tvEndereco"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtEndereco"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/telaDeCadastroDeUsuario_edtEndereco" />

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvTelefone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeUsuario_tvTelefone"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtTelefone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/telaDeCadastroDeUsuario_edtTelefone" />

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvRg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeUsuario_tvRg"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtRg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/telaDeCadastroDeUsuario_edtRg" />

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvCpf"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeUsuario_tvCpf"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtCpf"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/telaDeCadastroDeUsuario_edtCpf"/>

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvIdade"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeUsuario_tvIdade"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtIdade"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/telaDeCadastroDeUsuario_edtIdade" />

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvEscolaridade"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeUsuario_tvEscolaridade"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtEscolaridade"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/telaDeCadastroDeUsuario_edtEscolaridade"
                android:textAppearance="@android:style/TextAppearance.DialogWindowTitle" />

            <TextView
                android:id="@+id/telaDeCadastroDeParticipante_tvNumeroDeNis"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/telaDeCadastroDeUsuario_tvNumeroDeNis"
                android:textColor="@android:color/background_dark" />

            <EditText
                android:id="@+id/telaDeCadastroDeParticipante_edtNis"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="@string/telaDeCadastroDeUsuario_edtNis"
                android:textAppearance="@android:style/TextAppearance.DialogWindowTitle" />

            <Button
                android:id="@+id/telaDeCadastroDeParticipante_btnTelefone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/telaDecadastro_btnTelefone"
                android:layout_margin="3dp"
                android:onClick="ligar"
                android:layout_gravity="center"/>

        </LinearLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>
