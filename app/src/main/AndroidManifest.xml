    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CadastroDeLogin.TelaInicialActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroDeLogin.TelaDeCadastroActivity" />
        <activity android:name=".CadastroDeLogin.TelaDeLoginActivity" />
        <activity android:name=".CadastroDeGrupo.TelaDeCadastroDeGrupoActivity" />
        <activity android:name=".CadastroDeGrupo.TelaDeCadastroDeGrupoListActivity" />
        <activity android:name=".CadastroDeUsuario.TelaDeCadastroDeUsuarioActivity" />
